generator client{
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
     url = env("DATABASE_URL")
}

model book_stores {
    id Int @id @default(autoincrement())
    name String @db.VarChar(255) @unique
    address String?
    link String?
    subscribers Int @default(0)
    createdAt DateTime @default(now())
    updateAt DateTime @updatedAt
}

